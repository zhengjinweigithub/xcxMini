
.aut_v_c{
  position: fixed;
  width: 100%;
  height:100%;
  top:0;
  background-color: rgba(0, 0, 0, .5);
  z-index: 100
}
.aut_v{
  position: fixed;
  top: 32%;
  z-index: 101;
  background-color: #fff;
  text-align: center;
  width: 60%;
  font-size: 24rpx;
  left: 20%;
  padding: 80rpx 0 40rpx 0;
  border-radius: 10rpx;
  float: left;
}
.aut_v>text{
  font-weight: bold;
  font-size: 28rpx;
  float: left;
  width: 100%;
}
.aut_v>view{
  width: 80%;
  /* 转化元素类型 */
  display: grid;
  /* 设置网格的间隙 */
  grid-gap: 20rpx;
  /* 设置每列的尺寸 */
  grid-template-columns: repeat(2, 1fr);
  margin-left: 10%;
  margin-top: 80rpx;
}
.is_q{
  background-color: #DF9652;
  color: #fff;
  border-radius: 100rpx;
  width: 100%;
  /* margin-top: 50rpx; */
  padding: 0 !important;
  float: left;
  /* margin-left: 10%; */
  border: none !important;
}
.aut_v_c_image{
  width: 100%;
  height: 98rpx;
  position: fixed;
  bottom: 0;
  background-color: #fff;
}
.fg_white{
  width: 100%;
  height: 100%;
  background-color: #fff;
  position: fixed;
  top: 0;
  z-index: 2;
}