/* pages/products/productList/productList.wxss */
text,cover-view,view,scroll-view,textarea,input{
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}
scroll-view{
  overflow: scroll;
  -webkit-overflow-scrolling: touch;
}
::-webkit-scrollbar{
  width: 0 !important;
  height: 0 !important;
  color: transparent !important;
}
page{
  width: 100%;
  height: 100%;
  position: relative;
  background-color: #F5F5F5;
  float: left;
  padding: 0 !important;
}
.colorno2{
  color: #E8974D !important;
}
.filtrate-view{
  width: 100%;
  height: 100rpx;
  background-color: #fff;
}
.filtrate-view-view{
  width: 100%;
  height: 100rpx;
  background-color: #fff;
}
.filtrate-view-view > view {
  width: 33.3%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 28rpx;
  float: left;
}
.filtrate-view-view > view > image {
  width: 16rpx;
  height: 11rpx;
  margin: -2rpx 10rpx;
}

.guessLike-view{
  width: 100%;
  float: left;
  padding: 0 2%;
}
.guessLike-view-left,.guessLike-view-right{
  width: 50%;
  float: left;
  padding: 0 1%;
}
.guessLike-view-left>view,.guessLike-view-right>view{
  float: left;
}
.bottom_menu{
  width: 100%;
  position: fixed;
  bottom: 0;
  z-index: 100;
  background-color: #fff;
  color: #888888;
  font-size: 10px;
  text-align: center;
  height: 50px;
  border-top: 2rpx solid #ccc;
}
.bottom_menu>view{
  width: calc(100% / 5);
  height: 50px;
  float: left;
}
.bottom_menu>view>image{
  width: 24px;
  height: 24px;
  margin-top: 5px;
}
#recycleId{
  width: 100%;
}
.loadingcss{
  margin-top: 20rpx;
}